import leon.collection._
import leon.webDSL.webDescription._
import leon.webDSL.webBuilding._
import implicits._

object Main {
    //This function will be called by the evaluator. It has to return a WebPage
    def main() = {
      val hello = "Hello"
      val world = "World"
      val p = <.p(hello + " " + world)
      val h = <.h2("This is a test")
      val i = <.input(^.tpe := "button", ^.value := "Press me "+world+"!")
      val w = WebPage(List(), List(h, p, i))
      w
    }
}
